{
  "kind": "collectionType",
  "collectionName": "blog_posts",
  "info": {
    "singularName": "blog-post",
    "pluralName": "blog-posts",
    "displayName": "Blog Post",
    "description": "Blog-Beitr√§ge und Videos"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "titel": {
      "type": "string",
      "required": true
    },
    "inhalt": {
      "type": "richtext",
      "required": false
    },
    "autor": {
      "type": "string",
      "default": "Canaya",
      "required": true
    },
    "datum": {
      "type": "date",
      "required": true
    },
    "bild": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": ["images"]
    },
    "videoUrl": {
      "type": "string",
      "required": false
    },
    "tags": {
      "type": "json",
      "required": false
    },
    "kategorie": {
      "type": "enumeration",
      "enum": [
        "Tutorial",
        "Demo",
        "Herstellung",
        "Tipps",
        "News"
      ],
      "required": true
    },
    "featured": {
      "type": "boolean",
      "default": false
    },
    "sortierung": {
      "type": "integer",
      "default": 0
    }
  }
}
